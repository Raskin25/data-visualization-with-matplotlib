    "import numpy as np\n",
    "import matplotlib as mpl\n",
    "import matplotlib.pyplot as plt\n",
    "X=np.linspace(0, 2*np.pi, 100)\n",
    "Y=np.cos(X)\n",
    "fig, ax=plt.subplots()\n",
    "ax.plot(X,Y, color='green')\n",
    "fig.savefig(\"figure.pdf\")\n",
    "plt.show()"
   ]
  }
